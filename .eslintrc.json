{
    "env": {
        "node": true,
        "browser": true,
        "jest": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "parser": "babel-eslint",
    "plugins": ["import", "react", "jsx-ally"],
    "extends": [
        "eslint:recommended",
        "plugin:import/errors",
        "plugin:import/warnings",
        "plugin:react/recommended",
        "plugin:jsx-ally/recommended"
    ]  
}